{
    "version": 2,
    "rewrites": [
        {
            "source": "/",
            "destination": "/docs/viewer/app.html"
        },
        {
            "source": "/app.html",
            "destination": "/docs/viewer/app.html"
        },
        {
            "source": "/app.js",
            "destination": "/docs/viewer/app.js"
        },
        {
            "source": "/styles.css",
            "destination": "/docs/viewer/styles.css"
        },
        {
            "source": "/docs_index.json",
            "destination": "/docs/docs_index.json"
        }
    ],
    "headers": [
        {
            "source": "/(.*).js",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript"
                }
            ]
        },
        {
            "source": "/(.*).json",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/json"
                }
            ]
        }
    ]
}